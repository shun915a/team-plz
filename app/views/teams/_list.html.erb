  <div class="item-lists">
    <% if user_signed_in? %>
      <% @teams.each do |team| %>
        <%= react_component("TeamList", {post: team, tags: team.team_tags, userId: current_user.id, date: team.updated_at.strftime('%Y/%m/%d %H:%M')}) %>
      <% end %>
    <% else %>
      <% @teams.each do |team| %>
        <%= react_component("TeamList", {post: team, tags: team.team_tags, date: team.updated_at.strftime('%Y/%m/%d %H:%M')}) %>
      <% end %>
    <% end %>
  </div>